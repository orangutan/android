  <!-- junk projects, like for mac etc. -->
  <remove-project name="platform/prebuilts/gcc/darwin-x86/aarch64/aarch64-linux-android-4.9"/>
  <remove-project name="platform/prebuilts/gcc/darwin-x86/arm/arm-eabi-4.8"/>
